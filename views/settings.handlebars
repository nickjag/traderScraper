

<form action="./update" method="post" onsubmit="return false;">
	
	<div class="form-group">
		<label>Current Search: </label><br>
		{{{ searchTitle }}}<br><br>
	</div>
	<div class="form-group">
		<textarea id="url" class="form-control">{{{ searchURL }}}</textarea><br>
	</div>
	<div class="form-group">
		<input type="submit" id="update-url" class="btn btn-primary" value="Update">
	</div>
</form>